<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/FloatingActionButtonInicio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginStart="16dp"
        android:clickable="true"
        android:elevation="38dp"
        android:enabled="true"
        android:focusable="true"
        android:foregroundGravity="center"
        android:scaleType="center"
        android:backgroundTint="@color/colorAccent"
        android:src="@drawable/ic_ice_gota"
        app:fabCustomSize="60dp"
        app:fabSize="normal"
        app:layout_constraintStart_toStartOf="@+id/guideline5"
        app:layout_constraintTop_toTopOf="@+id/guideline2"
        app:maxImageSize="30dp" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/buyCellFloatingActionButtonText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:gravity="center"
        android:maxLines="3"
        android:text="Iniciar Visita\nRapida"
        android:textColor="@color/primaryTextColor"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/topUpRowFloatingActionButtonBrandFachada"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/FloatingActionButtonInicio" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButtonCaja"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:clickable="true"
        android:elevation="38dp"
        android:enabled="true"
        android:focusable="true"
        android:foregroundGravity="center"
        android:scaleType="center"
        android:backgroundTint="@color/colorAccent"
        android:src="@drawable/ic_ice_gota"
        app:fabCustomSize="60dp"
        app:fabSize="normal"
        app:layout_constraintEnd_toEndOf="@+id/topUpRowFloatingActionButtonBrandFachada"
        app:layout_constraintStart_toStartOf="@+id/topUpRowFloatingActionButtonBrandFachada"
        app:layout_constraintTop_toTopOf="@+id/floatingActionButtonBarra"
        app:maxImageSize="30dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/topUpRowFloatingActionButtonBrandFachada"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:clickable="true"
        android:elevation="38dp"
        android:enabled="true"
        android:focusable="true"
        android:foregroundGravity="center"
        android:scaleType="center"
        android:backgroundTint="@color/colorAccent"
        android:src="@drawable/ic_ice_gota"
        app:fabCustomSize="60dp"
        app:fabSize="normal"
        app:layout_constraintBottom_toBottomOf="@+id/FloatingActionButtonInicio"
        app:layout_constraintEnd_toStartOf="@+id/floatingButtonPiso"
        app:layout_constraintStart_toEndOf="@+id/FloatingActionButtonInicio"
        app:layout_constraintTop_toTopOf="@+id/FloatingActionButtonInicio"
        app:maxImageSize="30dp" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/buyCellFloatingActionButtonText2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:maxLines="3"
        android:text="Fachada"
        android:textColor="@color/primaryTextColor"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/topUpRowFloatingActionButtonBrandFachada"
        app:layout_constraintStart_toStartOf="@+id/topUpRowFloatingActionButtonBrandFachada"
        app:layout_constraintTop_toBottomOf="@+id/topUpRowFloatingActionButtonBrandFachada" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButtonBarra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:clickable="true"
        android:elevation="38dp"
        app:fabCustomSize="60dp"
        app:fabSize="normal"
        android:backgroundTint="@color/colorAccent"
        android:src="@drawable/ic_ice_gota"
        app:layout_constraintEnd_toEndOf="@+id/FloatingActionButtonInicio"
        app:layout_constraintStart_toStartOf="@+id/FloatingActionButtonInicio"
        app:layout_constraintTop_toBottomOf="@+id/buyCellFloatingActionButtonText"
        app:maxImageSize="30dp"
        app:srcCompat="@drawable/ic_abeja_naranja" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="#FFFFFF"
        android:gravity="center"
        android:text="Barra"
        android:textColor="@color/primaryTextColor"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/floatingActionButtonBarra"
        app:layout_constraintStart_toStartOf="@+id/floatingActionButtonBarra"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButtonBarra" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingButtonPiso"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:clickable="true"
        android:elevation="38dp"
        app:fabCustomSize="60dp"
        android:backgroundTint="@color/colorAccent"
        android:src="@drawable/ic_ice_gota"
        app:layout_constraintBottom_toBottomOf="@+id/topUpRowFloatingActionButtonBrandFachada"
        app:layout_constraintEnd_toStartOf="@+id/guideline6"
        app:layout_constraintTop_toTopOf="@+id/topUpRowFloatingActionButtonBrandFachada"
        app:maxImageSize="30dp"
        app:srcCompat="@drawable/ic_abeja_naranja" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="Piso de Venta"
        android:textColor="#101010"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/floatingButtonPiso"
        app:layout_constraintStart_toStartOf="@+id/floatingButtonPiso"
        app:layout_constraintTop_toBottomOf="@+id/floatingButtonPiso" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:gravity="center"
        android:text="Caja"
        android:textColor="#100F0F"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/floatingActionButtonCaja"
        app:layout_constraintStart_toStartOf="@+id/floatingActionButtonCaja"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButtonCaja" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingButtonPlan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="55dp"
        android:elevation="38dp"
        app:fabCustomSize="60dp"
        app:fabSize="normal"
        android:clickable="true"
        android:backgroundTint="@color/colorAccent"
        android:src="@drawable/ic_ice_gota"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintEnd_toEndOf="@+id/floatingActionButtonBarra"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/floatingActionButtonBarra"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:maxImageSize="30dp"
        app:srcCompat="@drawable/ic_abeja_naranja" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/imageButton8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="38dp"
        app:fabCustomSize="60dp"
        app:fabSize="normal"
        android:clickable="true"
        android:backgroundTint="@color/colorAccent"
        android:src="@drawable/ic_ice_gota"
        app:layout_constraintEnd_toEndOf="@+id/floatingActionButtonCaja"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/floatingActionButtonCaja"
        app:layout_constraintTop_toTopOf="@+id/floatingButtonPlan"
        app:maxImageSize="30dp"
        app:srcCompat="@drawable/ic_abeja_naranja" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/imageButton9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="38dp"
        app:fabCustomSize="60dp"
        app:fabSize="normal"
        android:clickable="true"
        android:backgroundTint="@color/colorAccent"
        android:src="@drawable/ic_ice_gota"
        app:layout_constraintEnd_toEndOf="@+id/floatingActionButtonBodega"
        app:layout_constraintStart_toStartOf="@+id/floatingActionButtonBodega"
        app:layout_constraintTop_toTopOf="@+id/imageButton8"
        app:maxImageSize="30dp"
        app:srcCompat="@drawable/ic_abeja_naranja" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButtonBodega"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:elevation="38dp"
        app:fabCustomSize="60dp"
        app:fabSize="normal"
        android:backgroundTint="@color/colorAccent"
        android:src="@drawable/ic_ice_gota"
        app:layout_constraintBottom_toBottomOf="@+id/floatingActionButtonCaja"
        app:layout_constraintEnd_toEndOf="@+id/floatingButtonPiso"
        app:layout_constraintStart_toStartOf="@+id/floatingButtonPiso"
        app:layout_constraintTop_toTopOf="@+id/floatingActionButtonCaja"
        app:maxImageSize="30dp"
        app:srcCompat="@drawable/ic_abeja_naranja" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="Bodega/Pizarra"
        android:textColor="#151414"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/floatingActionButtonBodega"
        app:layout_constraintStart_toStartOf="@+id/floatingActionButtonBodega"
        app:layout_constraintTop_toBottomOf="@+id/floatingActionButtonBodega" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="Plan de Trabajo"
        android:textColor="#0B0A0A"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/floatingButtonPlan"
        app:layout_constraintStart_toStartOf="@+id/floatingButtonPlan"
        app:layout_constraintTop_toBottomOf="@+id/floatingButtonPlan" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="Status Visita"
        android:textColor="#0E0D0D"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageButton8"
        app:layout_constraintStart_toStartOf="@+id/imageButton8"
        app:layout_constraintTop_toBottomOf="@+id/imageButton8" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="Salida Visita"
        android:textColor="#1E1C1C"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageButton9"
        app:layout_constraintStart_toStartOf="@+id/imageButton9"
        app:layout_constraintTop_toBottomOf="@+id/imageButton9" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="411dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintGuide_percent="0.12"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="411dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintGuide_percent="0.85"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.07" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.93" />


</androidx.constraintlayout.widget.ConstraintLayout>