<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nutrisaapplication">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:configChanges="orientation|keyboardHidden"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:screenOrientation="userPortrait"
        android:supportsRtl="true"
        android:requestLegacyExternalStorage="true"
        android:theme="@style/MaterialComponent">

        <activity
            android:name=".ui.main.basicoExterior.BasicoExteriorActivity"
            android:screenOrientation="userPortrait"
            android:theme="@style/AppTheme2" />
        <activity android:name=".ui.main.plan_trabajo.TableLayoutActivity" />
        <activity
            android:name=".ui.main.home_regional.view.HomeRegionalActivity"
            android:screenOrientation="userPortrait"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".ui.main.visitaLarga.VisitaLargaActivity"
            android:screenOrientation="userPortrait"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".ui.main.menuVisita.MenuActivity"
            android:screenOrientation="userPortrait"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".ui.main.quickVisit.QuickVisitActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="userPortrait"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".ui.main.supervision.SupervisionActivity"
            android:screenOrientation="userPortrait"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".ui.main.login.LoginActivity"
            android:screenOrientation="userPortrait"
            android:theme="@style/NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.main.home.HomeActivity"
            android:screenOrientation="userPortrait"
            android:theme="@style/MaterialComponent.NoActionBar" />
    </application>

</manifest>